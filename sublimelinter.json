{
	// "sublimelinter": "load-only",
	"sublimelinter": true,
	"jshint_options" :
	{
		"bitwise":  false,
		"newcap":   true,
		"eqeqeq":   true,
		"immed":    true,
		"nomen":    false,
		"onevar":   false,
		"plusplus": false,
		"regexp":   false,
		"strict":   false,
		"undef":    true,
		"white":    true,
		"debug":    false,
		"es5":      false,
		"evil":     false,
		"forin":    false,
		"laxbreak": false,
		"sub":      false,
		"maxlen":   160,
		"indent":   4,
		"maxerr":   100,
		"passfail": false,
		"predef":   [
			"$",
			"jQuery",
			"console",
			"Class",
			"Modernizr",
			"History",
			"RED",
			"red",
			"FB",
			"Scroller"
		],
		"browser":  true,
		"rhino":    false,
		"devel":    false,
		"loopfunc": true,
		"asi":      false,
		"boss":     false,
		"couch":    true,
		"curly":    true,
		"noarg":    true,
		"jquery":	true,
		"node":     true,
		"noempty":  false,
		"nonew":    true
	},
	"sublimelinter_syntax_map":
	{
	    "jQueryJavaScript": "javascript"
	},
	"sublimelinter_disable":
    [
    	"css"
    ]
}